                                                                {"id":"sbmi:input:32","op":">", "value":"3456"}
                                                            ]
                                                    },
                                                    {
                                                        "or":[
                                                                {"id":"sbmi:bits:124","op":"=","value":"0"},
                                                                {"id":"sbmi:bits:120","op":"=","value":"1"},
                                                                {"id":"sbmi:input:32","op":">","value":"3456"}
                                                            ]
                                                    }
                                                ],
                                                "action":"close_contactors:start"},
        {"trigger":"sbmi:bits:123=0", 
                            "enable":{
